CREATE TABLE health_data (
    gender VARCHAR(10),
    age NUMERIC,
    hypertension INTEGER,
    heart_disease INTEGER,
    smoking_history VARCHAR(20),
    bmi NUMERIC,
    HbA1c_level NUMERIC,
    blood_glucose_level INTEGER,
    stocks_pred INTEGER
);

'Open', 'High', 'Low', 'Close', 'Adj Close', 'Volume'

CREATE TABLE train_st (
    Open NUMERIC,
    High NUMERIC,
    Low NUMERIC,
    Close NUMERIC,
    Adj_Close NUMERIC,
    Volume NUMERIC,
    Up NUMERIC
);

CREATE TABLE test_st (
    Open NUMERIC,
    High NUMERIC,
    Low NUMERIC,
    Close NUMERIC,
    Adj_Close NUMERIC,
    Volume NUMERIC,
    Up NUMERIC
);

CREATE TABLE validate (
    gender VARCHAR(10),
    age NUMERIC,
    hypertension INTEGER,
    heart_disease INTEGER,
    smoking_history VARCHAR(20),
    bmi NUMERIC,
    HbA1c_level NUMERIC,
    blood_glucose_level INTEGER,
    stocks_pred INTEGER
);